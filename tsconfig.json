{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true, 
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "utils/getComparator.js",
    "app/api/auth/[...nextauth]/route.js"
, "tailwind.config.js", "components/Buttons/index.js", "app/api/mainPage/route.js", "components/Provider/index.js", "utils/uploadthing.js", "app/api/uploadthing/route.js", "app/api/uploadthing/core.js", "app/login/page.js"  ],
  "exclude": ["node_modules"]
}
